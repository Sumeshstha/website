{
  "name": "sumesh-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sumeshshrestha.com.np",
  "dependencies": {
    "axios": "^1.11.0",
    "framer-motion": "^11.11.17",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-pdf": "^10.2.0",
    "react-router-dom": "^7.9.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "react-scripts build && npx serve -s build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "clean": "rmdir /s /q build 2>nul & rmdir /s /q node_modules 2>nul & del package-lock.json 2>nul && npm install",
    "optimize": "npm run build && npx update-browserslist-db@latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
